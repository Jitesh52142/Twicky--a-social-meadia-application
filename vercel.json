{
  "version": 2,
  "name": "twicky_social_media",
  "builds": [
    {
      "src": "twiky_project/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "twiky_project/wsgi.py"
    }
  ],
  "env": {
    "VERCEL": "1",
    "DEBUG": "False",
    "ALLOWED_HOSTS": "localhost,127.0.0.1,.vercel.app"
  }
}

