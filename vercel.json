{
  "version": 2,
  "name": "twicky_social_media",
  "builds": [
    {
      "src": "twiky_project/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "twiky_project/wsgi.py"
    }
  ]
}

